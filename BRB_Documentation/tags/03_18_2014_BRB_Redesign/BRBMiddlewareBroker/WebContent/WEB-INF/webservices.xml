<?xml version="1.0" encoding="UTF-8"?>
<webservices xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/javaee_web_services_1_2.xsd" version="1.2">	
	<webservice-description>
		<webservice-description-name>PersistService1</webservice-description-name>
		<port-component>
			<port-component-name>PersistServiceDelegate</port-component-name>
			<wsdl-service xmlns:pfx="http://persistservice.BRB.ctfs.com/">
			pfx:PersistService</wsdl-service>
			<wsdl-port xmlns:pfx="http://persistservice.BRB.ctfs.com/">
			pfx:PersistServicePort</wsdl-port>
			<enable-mtom>false</enable-mtom>
			<service-endpoint-interface>com.ctfs.BRB.persistservice.PersistServiceDelegate</service-endpoint-interface>
			<service-impl-bean>
				<servlet-link>
				com.ctfs.BRB.persistservice.PersistServicePortBindingImpl</servlet-link>
			</service-impl-bean>
		</port-component>
	</webservice-description>
</webservices>
